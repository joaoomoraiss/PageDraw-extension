{
  "name": "PageDraw",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
        "matches":["<all_urls>"],
        "js": ["scripts/app.js"],
        "css": ["popup/style.css"],
        "run_at": "document_end"
    }
  ],
  "action": {
    "default_title": "Toggle Drawing Toolbar"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab","scripting"]
}